
launch_command="
docker run -it --rm -u $uid --name $name -e COLUMNS=$COLUMNS \
  -v $SIMPLETON_BASE:/repo \
  -v $SIMPLETON_HOME:/home \
  -v /Users/jgurr/work/one-infra-simpleton/work/low:/work/low \
  -v /Users/jgurr/work/one-infra-simpleton/work/one-data:/work/one-data \
  -v /Users/jgurr/work/simpleton/work/core:/work/core \
  -e prompt_name=\"$name \" \
  -e USER=$user \
  -e NO_PROXY= \
  -e HTTPS_PROXY= \
  -e HTTP_PROXY= \
  $name 
"
